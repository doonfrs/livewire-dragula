{
    "name": "trinavo/livewire-dragula",
    "description": "A Laravel Livewire integration for Dragula.js drag and drop library",
    "license": "MIT",
    "authors": [
        {
            "name": "Feras Abdulrahman",
            "email": "doonfrs@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "livewire/livewire": "^3.6",
        "laravel/helpers": "^1.7",
        "illuminate/support": "*"
    },
    "require-dev": {
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Trinavo\\LivewireDragula\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trinavo\\LivewireDragula\\LivewireDragulaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
